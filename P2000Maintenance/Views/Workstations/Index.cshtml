@model IEnumerable<P2000Maintenance.Models.P2000.Workstation>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Populate", "Populate")
</p> 
<p>
    @Html.ActionLink("Test Workstations", "TestStations")
</p>    
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Enabled)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Badging)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Server)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Version)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OnlineDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Online)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ip_Address)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PingTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Guid)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enabled)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Badging)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Server)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Version)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OnlineDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Online)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ip_Address)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PingTime) ms
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Guid)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
